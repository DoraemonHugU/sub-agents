# ============================================================
# Sub-Agent Framework Environment Configuration
# ============================================================
# Copy this file to `.env` and modify values as needed.

# Project working directory (where the agent will operate)
SUB_AGENT_CWD=/path/to/your/project

# Gemini CLI executable path (optional, auto-detected if not set)
# GEMINI_EXECUTABLE=gemini

# Enable preview/experimental models
USE_PREVIEW_MODELS=true

# Model aliases (typically no changes needed)
GEMINI_MODEL_ALIAS_AUTO=auto
GEMINI_MODEL_ALIAS_PRO=pro
GEMINI_MODEL_ALIAS_FLASH=flash
GEMINI_MODEL_ALIAS_FLASH_LITE=flash-lite

# Timeout for Gemini API calls (seconds)
GEMINI_TIMEOUT=360

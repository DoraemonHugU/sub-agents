{
    "mcpServers": {
        "sub-agents": {
            "command": "D:/your/project/.venv/Scripts/python.exe",
            "args": [
                "D:/your/project/subAgent/src/server.py"
            ],
            "env": {
                "SUB_AGENT_CWD": "D:/your/working/directory",
                "USE_PREVIEW_MODELS": "true",
                "GEMINI_MODEL_ALIAS_AUTO": "auto",
                "GEMINI_MODEL_ALIAS_PRO": "pro",
                "GEMINI_MODEL_ALIAS_FLASH": "flash",
                "GEMINI_MODEL_ALIAS_FLASH_LITE": "flash-lite",
                "GEMINI_TIMEOUT": "360"
            }
        }
    }
}
